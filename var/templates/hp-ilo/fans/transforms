cpqHeFltTolFanIdxTxt          : REGSUB : {cpqHeFltTolFanIndex} /(.*)/Fan#$1/
cpqHeFltTolFanPresentTxt      : SWITCH : {cpqHeFltTolFanPresent}  1=Other,2=Absent,3=OK
cpqHeFltTolFanLocaleTxt       : SWITCH : {cpqHeFltTolFanLocale} 1=Other,2=Unknown,3=System,4=System Board,5=IO Board,6=CPU,7=Memory,8=Storage,9=Removable Media,10=Power Supply,11=Ambient,12=Chassis,13=Bridge Card
cpqHeFltTolFanRedundantTxt    : SWITCH : {cpqHeFltTolFanRedundant}  1=Other,2=No,3=OK
cpqHeFltTolFanConditionTxt    : SWITCH : {cpqHeFltTolFanCondition} 1={cpqHeFltTolFanPresentTxt},2=OK,3=Degraded,4=Failed
cpqHeThermalSystemFanStatusTxt: SWITCH : {cpqHeThermalSystemFanStatus} 1=Other,2=OK,3=Degraded,4=Failed
cpqHeFltTolFanSpeedTxt        : SWITCH : {cpqHeFltTolFanSpeed}  1=Other,2=OK,3=High
